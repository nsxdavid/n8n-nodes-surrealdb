{
  "name": "n8n-nodes-surrealdb",
  "version": "0.1.0",
  "description": "n8n node to connect to SurrealDB",
  "keywords": [
    "n8n-community-node-package",
    "surrealdb",
    "n8n"
  ],
  "license": "MIT",
  "homepage": "https://n8n.io",
  "author": {
    "name": "David Whatley"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nsxdavid/n8n-nodes-surrealdb.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SurrealDbApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SurrealDb/SurrealDb.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.17.16",
    "@types/request-promise-native": "^1.0.18",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^9.26.0",
    "eslint-plugin-n8n-nodes-base": "^1.13.1",
    "gulp": "^4.0.2",
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "surrealdb": "^1.0.0"
  }
}
